# Server Configuration
NODE_ENV=development
PORT=3001
LOG_LEVEL=debug

# Autodesk Platform Services (APS) OAuth
APS_CLIENT_ID=your_aps_client_id_here
APS_CLIENT_SECRET=your_aps_client_secret_here
APS_CALLBACK_URL=http://localhost:3000/api/auth/callback
APS_BASE_URL=https://developer.api.autodesk.com

# Autodesk BIM 360 / ACC API
BIM360_API_BASE_URL=https://developer.api.autodesk.com/hq/v1

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/acc_user_manager
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Redis
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Session Management
SESSION_SECRET=change_this_to_a_random_secret_min_32_chars
SESSION_MAX_AGE=86400000

# Security
ENCRYPTION_KEY=change_this_to_a_random_32_byte_hex_key
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# APS API Rate Limits (per minute)
APS_RATE_LIMIT_PROJECTS=60
APS_RATE_LIMIT_USERS=100
APS_RATE_LIMIT_ROLES=100

# Job Queue
QUEUE_CONCURRENCY=3
JOB_TIMEOUT_MS=300000
JOB_MAX_RETRIES=3

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Logging
LOG_FILE_PATH=./logs/app.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=7
